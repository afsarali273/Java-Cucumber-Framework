{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User",
  "type": "object",
  "properties": {
    "id": { "type": "integer" },
    "name": { "type": "string" },
    "active": { "type": "boolean" },
    "email": { "type": "string", "format": "email" }
  },
  "required": ["id", "name", "active", "email"]
}

